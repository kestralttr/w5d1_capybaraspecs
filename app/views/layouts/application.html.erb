<!DOCTYPE html>
<html>
<head>
  <title>GoalApp</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>

  <% if current_user %>
  <ul>
    <li><%= current_user.username %>  <%= button_to "Logout", session_url, method: :delete %> </li>
  </ul>
  <% end %>

<%= yield %>

</body>
</html>
